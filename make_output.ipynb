   "source": [
    "def write_output(question, file_path):\n",
    "    answer = chatgpt(question)\n",
    "    answer = \"###\\n\" + answer + \"\\n\"\n",
    "    f = open(file_path, \"a\", encoding=\"UTF-8\")\n",
    "    f.write(answer)\n",
    "    f.close()"
   ]
  },
